{
  "spec": "./packages/**/__tests__/*.spec.ts",
  "require": "ts-node/register",
  "extension": [
    "ts"
  ],
  "inline-diffs ": false,
  "full-trace": true,
  "recursive": false,
  "diff": true,
  "exit": true,
  "slow": 75,
  "retries": 0,
  "timeout": 2000,
  "package": "./package.json",
  "reporter": "spec",
  "ignore": [
    "node_modules",
    "dist"
  ]
}
