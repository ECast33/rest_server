{
  "name": "imitate",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "start": "npm run dev",
    "tsnode": "export NODE_CONFIG_DIR=./packages/server/config&& ts-node-dev --no-notify -T --ignore=false -r tsconfig-paths/register ./packages/server/src/server.ts",
    "build": "lerna run build",
    "container": "npm run migrate_container_db && pm2 --no-daemon start build/server.js --name='IMITATE'"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/express-mysql-session": "^2.1.3",
    "@types/express-session": "^1.17.4",
    "@types/lodash": "^4.14.182",
    "@types/morgan": "^1.9.3",
    "@types/mysql": "^2.15.21",
    "@types/node": "^17.0.42",
    "@types/passport": "^1.0.9",
    "app-config": "^1.0.1",
    "barrelsby": "^2.3.4",
    "bcrypt": "^5.0.1",
    "bcrypt-nodejs": "^0.0.3",
    "body-parser": "^1.20.0",
    "custom-logger": "^0.3.1",
    "express": "^4.18.1",
    "express-mysql-session": "^2.1.8",
    "express-session": "^1.17.3",
    "lerna": "^5.1.1",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "ts-node": "^10.8.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.3",
    "yarn": "^1.22.19"
  }
}
